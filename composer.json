{
    "name": "nytris/boost",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Nytris\\Boost\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nytris\\Boost\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "asmblah/php-code-shift": "@dev",
        "psr/cache": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^10.2"
    },
    "config": {
        "platform": {
            "php": "8.1"
        },
        "sort-packages": true
    }
}
